<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="byui.cit360.familiarity.Hibernate.ContactEntity">
        <table name="contact" schema="cit360familiarity" catalog=""/>
        <id-class class="byui.cit360.familiarity.Hibernate.ContactEntityPK"/>
        <attributes>
            <id name="firstName">
                <column name="firstName" length="30"/>
            </id>
            <id name="lastName">
                <column name="lastName" length="30"/>
            </id>
            <id name="add1">
                <column name="add1" length="45"/>
            </id>
            <id name="add2">
                <column name="add2" length="45"/>
            </id>
            <id name="city">
                <column name="city" length="45"/>
            </id>
            <id name="state">
                <column name="state" length="15"/>
            </id>
            <id name="zip">
                <column name="zip" length="5"/>
            </id>
            <id name="tele">
                <column name="tele" length="20"/>
            </id>
        </attributes>
    </entity>
</entity-mappings>
