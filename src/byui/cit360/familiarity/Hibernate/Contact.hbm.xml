<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="byui.cit360.familiarity.Hibernate.Contact" table="contact" schema="cit360familiarity">
        <composite-id mapped="true" class="byui.cit360.familiarity.Hibernate.ContactPK">

            <key-property name="id">
                <column name="id" sql-type="int"/>
            </key-property>

        </composite-id>

        <property name="firstName">
            <column name="firstName" sql-type="varchar(30)" length="30"/>
        </property>

        <property name="lastName">
            <column name="lastName" sql-type="varchar(30)" length="30"/>
        </property>

        <property name="add1">
            <column name="add1" sql-type="varchar(45)" length="45"/>
        </property>

        <property name="city">
            <column name="city" sql-type="varchar(45)" length="45"/>
        </property>

        <property name="state">
            <column name="state" sql-type="varchar(15)" length="15"/>
        </property>

        <property name="zip">
            <column name="zip" sql-type="varchar(5)" length="5"/>
        </property>

        <property name="tele">
            <column name="tele" sql-type="varchar(20)" length="20"/>
        </property>

    </class>
</hibernate-mapping>